{
    "name": "wesmx/package-skeleton",
    "description": "A skeleton repository for Laravel packages.",
    "keywords": ["laravel", "skeleton", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wes Murray",
            "email": "octowes@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Wesmx\\PackageSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wesmx\\PackageSkeleton\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
